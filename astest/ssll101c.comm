            VALE_CALC=-1.3312371969411929e-05,
            VALE_REFE=-1.3869999999999999e-05,
            REFERENCE="SOURCE_EXTERNE",
            PRECISION=0.05,
            NOM_CAS="CHARGE NUMERO 1",
        ),
        _F(
            GROUP_NO="D",
