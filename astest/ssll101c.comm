            NOM_CMP="DZ",
            VALE_CALC=-8.032390518990907e-06,
            VALE_REFE=-8.0376000000000008e-06,
            REFERENCE="SOURCE_EXTERNE",
            PRECISION=1.0e-2,
            NOM_CAS="CHARGE NUMERO 1",
        ),
        _F(
            GROUP_NO="D",
            RESULTAT=RESU,
            NOM_CHAM="DEPL",
            NOM_CMP="DX",
            VALE_CALC=-0.00016411180749497425,
            VALE_REFE=-1.6445000000000001e-4,
            REFERENCE="SOURCE_EXTERNE",
            PRECISION=1.0e-2,
            NOM_CAS="CHARGE NUMERO 2",
        ),
        _F(
            GROUP_NO="D",
            RESULTAT=RESU,
            NOM_CHAM="DEPL",
            NOM_CMP="DY",
            VALE_CALC=-1.3679955453987833e-05,
            VALE_REFE=-1.4245e-05,
            REFERENCE="SOURCE_EXTERNE",
            PRECISION=0.05,
            NOM_CAS="CHARGE NUMERO 2",
        ),
        _F(
            GROUP_NO="D",
            RESULTAT=RESU,
            NOM_CHAM="DEPL",
            NOM_CMP="DZ",
            VALE_CALC=-1.0042450659368377e-05,
            VALE_REFE=-1.0047000000000001e-05,
            REFERENCE="SOURCE_EXTERNE",
            PRECISION=1.0e-2,
            NOM_CAS="CHARGE NUMERO 2",
        ),
        _F(
            NUME_ORDRE=1,
            GROUP_NO="D",
            RESULTAT=RESU1,
            NOM_CHAM="DEPL",
            NOM_CMP="DX",
            VALE_CALC=-0.006313258580781456,
            VALE_REFE=-6.3277000000000003e-3,
            REFERENCE="SOURCE_EXTERNE",
            PRECISION=0.01,
        ),
        _F(
            NUME_ORDRE=1,
            GROUP_NO="D",
            RESULTAT=RESU1,
            NOM_CHAM="DEPL",
            NOM_CMP="DY",
            VALE_CALC=-0.01310689508347406,
            VALE_REFE=-0.013091999999999999,
            REFERENCE="SOURCE_EXTERNE",
            PRECISION=1.0e-2,
        ),
        _F(
            NUME_ORDRE=1,
            GROUP_NO="D",
            RESULTAT=RESU1,
            NOM_CHAM="DEPL",
            NOM_CMP="DZ",
            VALE_CALC=0.016800968313735398,
            VALE_REFE=0.016798,
            REFERENCE="SOURCE_EXTERNE",
            PRECISION=1.0e-2,
        ),
    )
)

