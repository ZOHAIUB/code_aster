RESU = STAT_NON_LINE(
    MODELE=MOMECA,
    CHAM_MATER=CHMAT,
    EXCIT=(_F(CHARGE=CLMECA),),
    COMPORTEMENT=_F(RELATION="BETON_RAG"),
