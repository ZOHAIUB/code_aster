    MODELE=MOMECA,
    DDL_IMPO=(
        _F(GROUP_MA=("BAS", "HAUT"), DZ=0.0),
        _F(GROUP_MA=("COTEG", "COTED"), DY=0.0),
        _F(GROUP_MA=("ARRIERE", "AVANT"), DX=0.0),
    ),
)

