    CONVERGENCE=_F(ITER_GLOB_MAXI=20, RESI_GLOB_RELA=ResiGlobRela),
    SOLVEUR=_F(ACCELERATION="FR"),
)
RESU = CALC_CHAMP(
    RESULTAT=RESU, reuse=RESU, TOUT="OUI", TOUT_ORDRE="OUI", DEFORMATION=("EPSI_ELGA",)
