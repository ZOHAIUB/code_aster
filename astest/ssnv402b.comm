NbTfin = 100
L_INST = DEFI_LIST_REEL(
    DEBUT=0.0, INTERVALLE=(_F(JUSQU_A=2.0, NOMBRE=10), _F(JUSQU_A=Tfin, NOMBRE=99))
)

