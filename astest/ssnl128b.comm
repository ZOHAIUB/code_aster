#   GRAN_IRRA_LOG DEPENDANT DE LA FLUENCE ET GRANDISSEMENT

DEBUT(CODE="OUI", ERREUR=_F(ALARME="EXCEPTION"), DEBUG=_F(SDVERI="OUI"))

from ssnl128b_mater import *

MA = LIRE_MAILLAGE(FORMAT="MED")

MA = DEFI_GROUP(reuse=MA, MAILLAGE=MA, CREA_GROUP_MA=_F(NOM="TOUT", TOUT="OUI"))

MO = AFFE_MODELE(MAILLAGE=MA, AFFE=_F(TOUT="OUI", PHENOMENE="MECANIQUE", MODELISATION="3D"))

CHXN = CREA_CHAMP(TYPE_CHAM="NOEU_GEOM_R", OPERATION="EXTR", MAILLAGE=MA, NOM_CHAM="GEOMETRIE")

FLUXAX = DEFI_FONCTION(NOM_PARA="X", VALE=(0.0, 1e-5, 4000.0, 1e-5))

FLUX_Y = DEFI_FONCTION(NOM_PARA="Y", VALE=(0.0, 1.0, 100.0, 1.0))

FLUXPL = DEFI_NAPPE(NOM_PARA="Z", PARA=(0.0, 100.0), FONCTION=(FLUX_Y, FLUX_Y))
# ------------------------------------------------
# CREATION DU CHAMP ASSOCIE A LA FONCTION FLUXAX
# ------------------------------------------------

CH_FAX = CREA_CHAMP(
    TYPE_CHAM="NOEU_NEUT_F",
    OPERATION="AFFE",
    MAILLAGE=MA,
    AFFE=_F(TOUT="OUI", NOM_CMP="X1", VALE_F=FLUXAX),
)

CH_FAX_R = CREA_CHAMP(TYPE_CHAM="NOEU_NEUT_R", OPERATION="EVAL", CHAM_F=CH_FAX, CHAM_PARA=CHXN)
# -----------------------------------------------
# CREATION DU CHAMP ASSOCIE A LA FONCTION FLUXPL
# -----------------------------------------------

CH_FPL = CREA_CHAMP(
    TYPE_CHAM="NOEU_NEUT_F",
    OPERATION="AFFE",
    MAILLAGE=MA,
    AFFE=_F(TOUT="OUI", NOM_CMP="X2", VALE_F=FLUXPL),
)

CH_FPL_R = CREA_CHAMP(TYPE_CHAM="NOEU_NEUT_R", OPERATION="EVAL", CHAM_F=CH_FPL, CHAM_PARA=CHXN)
# ------------------
# FONCTION PRODUIT
# ------------------

MULT = FORMULE(VALE="X1*X2*INST", NOM_PARA=["X1", "X2", "INST"])

CHRES = CREA_CHAMP(
    TYPE_CHAM="NOEU_NEUT_F",
    OPERATION="AFFE",
    MAILLAGE=MA,
    AFFE=_F(TOUT="OUI", NOM_CMP="X1", VALE_F=MULT),
)
# -----------------------------------------------------
# CREATION DU CHAMP FLUENC1 ASSOCIE A LA LISTE LINST
# LE CHAMP EST CALCULE POUR LES 5 PREMIERS INSTANTS
# -----------------------------------------------------
#

INST0_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_INST_R",
    OPERATION="AFFE",
    MAILLAGE=MA,
    AFFE=_F(TOUT="OUI", NOM_CMP="INST", VALE=0.0),
)

REST0_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_NEUT_R", OPERATION="EVAL", CHAM_F=CHRES, CHAM_PARA=(CH_FAX_R, CH_FPL_R, INST0_1)
)

RES00_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_IRRA_R",
    OPERATION="ASSE",
    MAILLAGE=MA,
    ASSE=_F(TOUT="OUI", CHAM_GD=REST0_1, NOM_CMP="X1", NOM_CMP_RESU="IRRA"),
    INFO=2,
)
#
#
#

INST1_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_INST_R",
    OPERATION="AFFE",
    MAILLAGE=MA,
    AFFE=_F(TOUT="OUI", NOM_CMP="INST", VALE=1.0),
)

REST1_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_NEUT_R", OPERATION="EVAL", CHAM_F=CHRES, CHAM_PARA=(CH_FAX_R, CH_FPL_R, INST1_1)
)

RES11_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_IRRA_R",
    OPERATION="ASSE",
    MAILLAGE=MA,
    ASSE=_F(TOUT="OUI", CHAM_GD=REST1_1, NOM_CMP="X1", NOM_CMP_RESU="IRRA"),
    INFO=2,
)
#
#

INST2_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_INST_R",
    OPERATION="AFFE",
    MAILLAGE=MA,
    AFFE=_F(TOUT="OUI", NOM_CMP="INST", VALE=864.99),
)

REST2_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_NEUT_R", OPERATION="EVAL", CHAM_F=CHRES, CHAM_PARA=(CH_FAX_R, CH_FPL_R, INST2_1)
)

RES22_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_IRRA_R",
    OPERATION="ASSE",
    MAILLAGE=MA,
    ASSE=_F(TOUT="OUI", CHAM_GD=REST2_1, NOM_CMP="X1", NOM_CMP_RESU="IRRA"),
    INFO=2,
)
#
#

INST3_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_INST_R",
    OPERATION="AFFE",
    MAILLAGE=MA,
    AFFE=_F(TOUT="OUI", NOM_CMP="INST", VALE=1728.98),
)

REST3_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_NEUT_R", OPERATION="EVAL", CHAM_F=CHRES, CHAM_PARA=(CH_FAX_R, CH_FPL_R, INST3_1)
)

RES33_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_IRRA_R",
    OPERATION="ASSE",
    MAILLAGE=MA,
    ASSE=_F(TOUT="OUI", CHAM_GD=REST3_1, NOM_CMP="X1", NOM_CMP_RESU="IRRA"),
    INFO=2,
)
#
#

INST4_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_INST_R",
    OPERATION="AFFE",
    MAILLAGE=MA,
    AFFE=_F(TOUT="OUI", NOM_CMP="INST", VALE=2592.97),
)

REST4_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_NEUT_R", OPERATION="EVAL", CHAM_F=CHRES, CHAM_PARA=(CH_FAX_R, CH_FPL_R, INST4_1)
)

RES44_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_IRRA_R",
    OPERATION="ASSE",
    MAILLAGE=MA,
    ASSE=_F(TOUT="OUI", CHAM_GD=REST4_1, NOM_CMP="X1", NOM_CMP_RESU="IRRA"),
    INFO=2,
)
#
#

INST5_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_INST_R",
    OPERATION="AFFE",
    MAILLAGE=MA,
    AFFE=_F(TOUT="OUI", NOM_CMP="INST", VALE=3456.96),
)

REST5_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_NEUT_R", OPERATION="EVAL", CHAM_F=CHRES, CHAM_PARA=(CH_FAX_R, CH_FPL_R, INST5_1)
)

RES55_1 = CREA_CHAMP(
    TYPE_CHAM="NOEU_IRRA_R",
    OPERATION="ASSE",
    MAILLAGE=MA,
    ASSE=_F(TOUT="OUI", CHAM_GD=REST5_1, NOM_CMP="X1", NOM_CMP_RESU="IRRA"),
    INFO=2,
)
#
#

FLUENC1 = CREA_RESU(
    OPERATION="AFFE",
    TYPE_RESU="EVOL_VARC",
    AFFE=(
        _F(NOM_CHAM="IRRA", CHAM_GD=RES00_1, INST=0.0),
        _F(NOM_CHAM="IRRA", CHAM_GD=RES11_1, INST=1.0),
        _F(NOM_CHAM="IRRA", CHAM_GD=RES22_1, INST=864.99),
        _F(NOM_CHAM="IRRA", CHAM_GD=RES33_1, INST=1728.98),
        _F(NOM_CHAM="IRRA", CHAM_GD=RES44_1, INST=2592.97),
        _F(NOM_CHAM="IRRA", CHAM_GD=RES55_1, INST=3456.96),
        _F(NOM_CHAM="IRRA", CHAM_GD=RES55_1, INST=5000.0),
    ),
)
FIN()
